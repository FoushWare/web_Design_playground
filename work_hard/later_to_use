    <script>
      var mn = $(".main-nav");

      $(window).scroll(function () {
        if( $(this).scrollTop() > 250 ) {
          mn.addClass("main-nav-scrolled");
        } else {
          mn.removeClass("main-nav-scrolled");
        }
      });
    //@ sourceURL=pen.js
    </script>
